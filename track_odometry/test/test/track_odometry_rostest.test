<?xml version="1.0"?>
<launch>
  <executable name="test_track_odometry" cmd="./build/track_odometry/test/test_track_odometry" output="screen"/>

  <node pkg="track_odometry" exec="track_odometry" name="track_odometry">
    <param name="base_link_id" value="base_link" />
    <param name="use_kf" value="false" />
    <param name="z_filter_timeconst" value="-1.0" />
  </node>

  <group>
    <node pkg="track_odometry" exec="track_odometry" name="track_odometry_no_z_filter" namespace="no_z_filter">
      <param name="base_link_id" value="base_link" />
      <param name="use_kf" value="false" />
      <param name="z_filter_timeconst" value="-1.0" />
    </node>
  </group>
  <group>
    <node pkg="track_odometry" exec="track_odometry" name="track_odometry_no_z_filter" namespace="no_z_filter_old_param">
      <param name="base_link_id" value="base_link" />
      <param name="use_kf" value="false" />
      <param name="z_filter" value="1.0" />
    </node>
  </group>
  <group>
    <node pkg="track_odometry" exec="track_odometry" name="track_odometry_z_filter" namespace="z_filter">
      <param name="base_link_id" value="base_link" />
      <param name="use_kf" value="false" />
      <param name="z_filter_timeconst" value="2.0" />
    </node>
  </group>
  <group>
    <node pkg="track_odometry" exec="track_odometry" name="track_odometry_z_filter" namespace="z_filter_old_param">
      <param name="base_link_id" value="base_link" />
      <param name="use_kf" value="false" />
      <param name="z_filter" value="0.995" />
    </node>
  </group>
</launch>
