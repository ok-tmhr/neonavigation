<?xml version="1.0"?>
<launch>
  <executable name="test_joystick_interrupt" cmd="./build/joystick_interrupt/test/test_joystick_interrupt" output="screen" />

  <node pkg="joystick_interrupt" exec="joystick_interrupt" name="joystick_interrupt">
    <param name="interrupt_button" value="0" />
    <param name="high_speed_button" value="1" />
    <param name="linear_axis" value="0" />
    <param name="angular_axis" value="1" />
    <param name="linear_axis2" value="2" />
    <param name="angular_axis2" value="3" />
    <param name="linear_vel" value="1.0" />
    <param name="angular_vel" value="1.0" />
    <param name="linear_high_speed_ratio" value="2.0" />
    <param name="angular_high_speed_ratio" value="2.0" />
  </node>
  <node pkg="joystick_interrupt" exec="joystick_interrupt" name="joystick_interrupt_omni">
    <param name="interrupt_button" value="0" />
    <param name="high_speed_button" value="1" />
    <param name="linear_axis" value="0" />
    <param name="angular_axis" value="1" />
    <param name="linear_axis2" value="2" />
    <param name="angular_axis2" value="3" />
    <param name="linear_vel" value="1.0" />
    <param name="angular_vel" value="1.0" />
    <param name="linear_high_speed_ratio" value="2.0" />
    <param name="angular_high_speed_ratio" value="2.0" />
    <param name="linear_y_vel" value="0.5" />
    <param name="linear_y_axis" value="4" />
    <param name="linear_y_axis2" value="5" />
    <remap from="cmd_vel" to="cmd_vel_omni"/>
  </node>
  <!-- <node pkg="joystick_interrupt" exec="joystick_mux" name="joystick_mux">
    <param name="interrupt_button" value="0" />
  </node> -->
</launch>
